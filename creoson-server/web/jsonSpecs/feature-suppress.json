{
  "spec" : {
    "function_description" : "Suppress one or more features that match criteria",
    "command" : "feature",
    "function" : "suppress",
    "notes" : [ "Will only suppress visible features." ],
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "File name",
      "default" : "The currently active model",
      "wildcards_allowed" : true
    }, {
      "name" : "name",
      "type" : "string",
      "description" : "Feature name",
      "default" : "All feature names",
      "wildcards_allowed" : true
    }, {
      "name" : "status",
      "type" : "string",
      "description" : "Feature status pattern",
      "default" : "All feature statuses",
      "wildcards_allowed" : true,
      "valid_values" : "ACTIVE, INACTIVE, FAMILY_TABLE_SUPPRESSED, SIMP_REP_SUPPRESSED, PROGRAM_SUPPRESSED, SUPPRESSED, UNREGENERATED"
    }, {
      "name" : "type",
      "type" : "string",
      "description" : "Feature type pattern",
      "default" : "All feature types",
      "wildcards_allowed" : true
    }, {
      "name" : "clip",
      "type" : "boolean",
      "description" : "Whether to clip-suppress ANY features from this feature through the end of the structure",
      "default" : "true"
    }, {
      "name" : "with_children",
      "type" : "boolean",
      "description" : "Whether to suppress any child features of the suppressed feature",
      "default" : "true"
    } ],
    "response" : [ ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "suppress",
      "data" : {
        "file" : "box.prt",
        "name" : "CS3"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "suppress",
      "data" : {
        "file" : "box.prt",
        "name" : "PLN_*",
        "clip" : true
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "suppress",
      "data" : {
        "file" : "box.prt",
        "status" : "*ACTIVE*",
        "with_children" : true
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "suppress",
      "data" : {
        "file" : "box.prt",
        "name" : "PLN_*",
        "status" : "ACTIVE"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "suppress",
      "data" : {
        "file" : "box.prt",
        "type" : "DATUM_*"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  } ]
}