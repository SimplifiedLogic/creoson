{
  "spec" : {
    "function_description" : "Resume one or more features that match criteria",
    "command" : "feature",
    "function" : "resume",
    "notes" : [ "Will only resume visible features." ],
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "File name",
      "default" : "The currently active model",
      "wildcards_allowed" : true
    }, {
      "name" : "name",
      "type" : "string",
      "description" : "Feature name; only used if names is not given",
      "wildcards_allowed" : true
    }, {
      "name" : "names",
      "type" : "array:string",
      "description" : "List of feature names",
      "default" : "The name parameter is used; if both are empty, then all features may be resumed"
    }, {
      "name" : "status",
      "type" : "string",
      "description" : "Feature status pattern",
      "default" : "All feature statuses",
      "wildcards_allowed" : true,
      "valid_values" : "ACTIVE, INACTIVE, FAMILY_TABLE_SUPPRESSED, SIMP_REP_SUPPRESSED, PROGRAM_SUPPRESSED, SUPPRESSED, UNREGENERATED"
    }, {
      "name" : "type",
      "type" : "string",
      "description" : "Feature type pattern",
      "default" : "All feature types",
      "wildcards_allowed" : true
    }, {
      "name" : "with_children",
      "type" : "boolean",
      "description" : "Whether to resume any child features of the resumed feature",
      "default" : "true"
    } ],
    "response" : [ ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "box.prt",
        "name" : "CS3"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "box.prt",
        "name" : "PLN_*"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "box.prt",
        "status" : "*SUPPRESSED*",
        "with_children" : true
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "box.prt",
        "name" : "PLN_*",
        "status" : "*SUPPRESSED*"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "box.prt",
        "type" : "DATUM_*"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "abc123.asm",
        "name" : "bolt.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "feature",
      "function" : "resume",
      "data" : {
        "file" : "abc123.asm",
        "names" : [ "bolt.prt", "nut.prt" ]
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  } ]
}