{
  "spec" : {
    "function_description" : "Check whether a file is checked out in a workspace",
    "command" : "windchill",
    "function" : "file_checked_out",
    "request" : [ {
      "name" : "workspace",
      "type" : "string",
      "description" : "Workspace name",
      "required" : true
    }, {
      "name" : "filename",
      "type" : "string",
      "description" : "File name",
      "required" : true
    } ],
    "response" : [ {
      "name" : "checked_out",
      "type" : "boolean",
      "description" : "Whether the file is checked out in the workspace"
    } ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "windchill",
      "function" : "file_checked_out",
      "data" : {
        "workspace" : "freds_workspace",
        "filename" : "box.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "checked_out" : true
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "windchill",
      "function" : "file_checked_out",
      "data" : {
        "workspace" : "freds_workspace",
        "filename" : "box-x.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "checked_out" : false
      }
    }
  } ]
}