{
  "spec" : {
    "function_description" : "Get a list of dimension details from a model",
    "command" : "dimension",
    "function" : "list_detail",
    "notes" : [ "If select is true, then the current selection in Creo will be cleared even if no items are found.", "See the documentation for object : DimDetailData to find out which values are returned for drawings vs models." ],
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "File name",
      "default" : "The currently active model"
    }, {
      "name" : "name",
      "type" : "string",
      "description" : "Dimension name; only used if names is not given",
      "wildcards_allowed" : true
    }, {
      "name" : "names",
      "type" : "array:string",
      "description" : "List of dimension names",
      "default" : "The name parameter is used; if both are empty, then all dimensions are listed"
    }, {
      "name" : "dim_type",
      "type" : "string",
      "description" : "Dimension type filter",
      "default" : "no filter",
      "valid_values" : "linear, radial, diameter, angular"
    }, {
      "name" : "encoded",
      "type" : "boolean",
      "description" : "Whether to return the values Base64-encoded",
      "default" : "false"
    }, {
      "name" : "select",
      "type" : "boolean",
      "description" : "If true, the dimensions that are found will be selected in Creo",
      "default" : "false"
    } ],
    "response" : [ {
      "name" : "dimlist",
      "type" : "object_array:DimDetailData",
      "description" : "List of dimension information"
    } ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "dimension",
      "function" : "list_detail",
      "data" : {
        "file" : "box.prt",
        "name" : "d*1"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "dimlist" : [ {
          "name" : "d21",
          "value" : 32.5,
          "encoded" : false,
          "dwg_dim" : false,
          "dim_type" : "linear",
          "text" : [ "{0:@D}\n" ]
        }, {
          "name" : "d31",
          "value" : 5.0,
          "encoded" : false,
          "dwg_dim" : false,
          "dim_type" : "linear",
          "text" : [ "{0:@D}\n" ]
        } ]
      }
    }
