{
  "spec" : {
    "function_description" : "Set the current material for a part or parts",
    "command" : "file",
    "function" : "set_cur_material",
    "notes" : [ "If 'material' has a file extension, it will be removed before the material is set." ],
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "Part name",
      "default" : "Currently active model",
      "wildcards_allowed" : true
    }, {
      "name" : "material",
      "type" : "string",
      "description" : "Material name",
      "required" : true
    } ],
    "response" : [ {
      "name" : "files",
      "type" : "array:string",
      "description" : "List of models which had the material set"
    } ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "set_cur_material",
      "data" : {
        "file" : "box.prt",
        "material" : "brass"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "files" : [ "box.prt" ]
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "set_cur_material",
      "data" : {
        "file" : "*.prt",
        "material" : "brass"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "files" : [ "box.prt", "lid.prt", "handle.prt" ]
      }
    }
  } ]
}