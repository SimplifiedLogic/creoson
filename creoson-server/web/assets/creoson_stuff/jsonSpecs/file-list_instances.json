{
  "spec" : {
    "function_description" : "List instances in a models family table",
    "command" : "file",
    "function" : "list_instances",
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "File name",
      "default" : "Currently active model"
    } ],
    "response" : [ {
      "name" : "dirname",
      "type" : "string",
      "description" : "Directory name of the file"
    }, {
      "name" : "generic",
      "type" : "string",
      "description" : "Generic name"
    }, {
      "name" : "files",
      "type" : "array:string",
      "description" : "List of model names in the table"
    } ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "list_instances",
      "data" : {
        "file" : "bracket.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "dirname" : "C:/myfiles/parts",
        "generic" : "bracket",
        "files" : [ "wide_bracket.prt", "thick_bracket.prt", "wide_thick_bracket.prt" ]
      }
    }
  } ]
}